{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "lib": ["DOM",  "ES2021"],
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "esModuleInterop":true,
      "paths": {
        "@/*": ["src/*"]
        },
        "baseUrl": "."
    },
    "include": ["./src/**/*"]
  }
